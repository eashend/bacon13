# Firebase Configuration - Copy to .env and fill in real values
# DO NOT commit the .env file to git!

# Firebase Web Configuration
FIREBASE_API_KEY=your-web-api-key-here
FIREBASE_APP_ID=your-web-app-id-here
FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id-here
FIREBASE_PROJECT_ID=your-project-id-here
FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
FIREBASE_STORAGE_BUCKET=your-project-id.firebasestorage.app

# Firebase Android Configuration
FIREBASE_API_KEY_ANDROID=your-android-api-key-here
FIREBASE_APP_ID_ANDROID=your-android-app-id-here

# Firebase iOS Configuration
FIREBASE_API_KEY_IOS=your-ios-api-key-here
FIREBASE_APP_ID_IOS=your-ios-app-id-here
FIREBASE_APP_ID_MACOS=your-macos-app-id-here
FIREBASE_IOS_BUNDLE_ID=com.example.bacon13